<nav class="navbar navbar-expand-lg navbar-light bg-light">
    <a class="navbar-brand" href="#">My Reviews</a>
</nav>

<div>
    <h2>My reviewed submissions</h2>
    <ul>
        {{#each reviewed}}
        <li><a href="/cfp/{{this.cfpId}}">{{this.title}}</a></li>
        {{/each}}
    </ul>
    <h2>Submissions I cannot review</h2>
    <ul>
        {{#each refused}}
        <li><a href="/cfp/{{this.cfpId}}">{{this.title}}</a></li>
        {{/each}}
    </ul>
</div>
